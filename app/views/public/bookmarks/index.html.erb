
  <div class="container">
  <div class="row">
    <div class="col-md-12">
      <h4 class="headline mt-5 ml-5">いいねした投稿</h4>
        <div class="row mt-3">
        <% @bookmark_posts.each do |bookmark| %>
          <%= bookmark.post.user.name %>
        <%= link_to post_path(bookmark.post) do %>
          <%= bookmark.post.title %>
        <% end %>
        <%= bookmark.post.body %>
      </div>
      <div class="d-flex justify-content-center">
        <%= paginate @bookmark_posts, theme: 'bootstrap-5' %>
      </div>
    </div>
  </div>
</div>